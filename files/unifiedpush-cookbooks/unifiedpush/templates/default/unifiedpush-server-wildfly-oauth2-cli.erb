embed-server --server-config=standalone-full-ha.xml
/system-property=aerogear.config.oauth2.enable:add(value=<%= @oauth2_enabled %>)
/system-property=aerogear.config.oauth2.ups.realm.name:add(value=unifiedpush)
/system-property=aerogear.config.oauth2.upsi.realm.name:add(value=unifiedpush-installations)
/system-property=aerogear.config.oauth2.ups.admin.client:add(value=admin-cli)
/system-property=aerogear.config.oauth2.admin.username:add(value=<%= @oauth2_admin_user %>)
/system-property=aerogear.config.oauth2.admin.password:add(value=<%= @oauth2_admin_pass %>)
/system-property=aerogear.config.oauth2.enforce.rooturl.domain:add(value=<%= @webapp_host %>)
/system-property=aerogear.config.oauth2.enforce.rooturl.protocol:add(value=http<% if @server_https %>s<% end %>)
/system-property=aerogear.config.oauth2.enforce.rooturl.seperator:add(value=<%= @oauth2_subdomain_seperator %>)
