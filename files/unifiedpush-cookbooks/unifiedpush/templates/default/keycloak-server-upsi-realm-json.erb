{
    "realm": "unifiedpush-installations",
    "enabled": true,
    "accessTokenLifespan": 60,
    "accessCodeLifespan": 60,
    "accessCodeLifespanUserAction": 300,
    "ssoSessionIdleTimeout": 600,
    "ssoSessionMaxLifespan": 36000,
    "sslRequired": "external",
    "registrationAllowed": false,
    "social": false,
    "adminTheme": "aerogear",
    "accountTheme": "unifiedpush-installations",
    "loginTheme": "unifiedpush-installations",
    "updateProfileOnInitialSocialLogin": false,
    "requiredCredentials": [ "password" ],
    "defaultRoles": [ "installation" ],
    "users" : [
        {
            "username" : "admin",
            "enabled": true,
            "credentials" : [{ "type" : "password", "value" : "password" }],
            "realmRoles": [ "admin" ],
            "applicationRoles": {
               "realm-management": [ "realm-admin" ],
               "account": [ "manage-account" ]
            }
        }
    ],
    "roles" : {
        "realm" : [
            {
                "name": "admin",
                "description": "Administrator privileges"
            },
            {
                "name": "installation",
                "description": "Device installation privileges"
            }
        ]
    },
    "scopeMappings": [ ],
    "clients": [
        {
            "clientId": "ups-installation-c-b4-test",
            "enabled": true,
	    "publicClient": true,
	    "rootUrl": "http<% if @server_https %>s<% end %>://c-b4-test<%= @oauth2_subdomain_seperator %><%= @server_host %>"
            "baseUrl": "/",
            "redirectUris": [
                "http://localhost",
                "/*"
	    ],
	    "webOrigins": [
                "*"
      	    ]
        }
    ]
}
